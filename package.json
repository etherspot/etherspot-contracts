  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "lint:js": "eslint ./{migrations,scripts,test} --ext .js",
    "lint:sol": "solhint ./src/**/*.sol ./src/**/**/*.sol",
    "lint": "npm run lint:sol",
    "compile": "truffle compile",
    "compile:all": "truffle compile --all",
    "migrate": "truffle migrate --network=default",
    "migrate:reset": "truffle migrate --network=default --reset",
    "test": "./scripts/test.sh --network test",
    "test:log-gas-usage": "cross-env LOG_GAS_USAGE=1 ./scripts/test.sh --network test",
    "test:compile-all": "./scripts/test.sh --network test --compile-all",
    "build": "node ./scripts/build",
    "initialize": "node ./scripts/initialize",
    "generate-events:account-registry": "node ./scripts/generate-events account-registry",
    "generate-events:message-hash-registry": "node ./scripts/generate-events message-hash-registry"
  },
  "dependencies": {},
  "devDependencies": {
    "@truffle/hdwallet-provider": "1.0.34",
    "@types/bn.js": "4.11.6",
    "@types/fs-extra": "8.1.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.2",
    "@types/web3": "1.0.20",
    "bn.js": "4.11.8",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "eth-sig-util": "2.5.3",
    "expect": "25.1.0",
    "fs-extra": "9.0.0",
    "ganache-cli": "6.9.1",
    "solhint": "^3.0.0-rc.8",
    "triple-beam": "1.3.0",
    "truffle": "5.1.22",
    "truffle-privatekey-provider": "1.3.0",
    "web3": "1.2.1",
    "winston": "3.2.1"
  }
}
