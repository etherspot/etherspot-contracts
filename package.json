{
  "name": "@etherspot/contracts",
  "version": "1.0.0-beta.1",
  "description": "ETHERspot contracts",
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "lint-js": "eslint ./{migrations,scripts,test} --ext .js",
    "lint-sol": "solhint ./src/**/*.sol ./src/**/**/*.sol",
    "lint": "npm run lint-sol",
    "compile": "truffle compile",
    "compile-all": "truffle compile --all",
    "test": "./scripts/test.sh",
    "test-log-gas-usage": "./scripts/test.sh --log-gas-usage",
    "migrate:mainnet": "truffle migrate --network=mainnet",
    "migrate:ropsten": "truffle migrate --network=ropsten",
    "migrate:rinkeby": "truffle migrate --network=rinkeby",
    "migrate:goerli": "truffle migrate --network=goerli",
    "migrate:kovan": "truffle migrate --network=kovan",
    "migrate:xdai": "truffle migrate --network=xdai",
    "migrate:sokol": "truffle migrate --network=sokol",
    "migrate:bsc": "truffle migrate --network=bsc",
    "migrate:bsc_test": "truffle migrate --network=bsc_test",
    "migrate:local_a": "truffle migrate --network=local_a",
    "migrate:local_b": "truffle migrate --network=local_b",
    "build": "node ./scripts/build",
    "initialize:mainnet": "node ./scripts/initialize mainnet",
    "initialize:ropsten": "node ./scripts/initialize ropsten",
    "initialize:rinkeby": "node ./scripts/initialize rinkeby",
    "initialize:goerli": "node ./scripts/initialize goerli",
    "initialize:kovan": "node ./scripts/initialize kovan",
    "initialize:xdai": "node ./scripts/initialize xdai",
    "initialize:sokol": "node ./scripts/initialize sokol",
    "initialize:bsc": "node ./scripts/initialize bsc",
    "initialize:bsc_test": "node ./scripts/initialize bsc_test",
    "initialize:local_a": "node ./scripts/initialize local_a",
    "initialize:local_b": "node ./scripts/initialize local_b"
  },
  "dependencies": {},
  "devDependencies": {
    "@truffle/hdwallet-provider": "1.0.42",
    "@types/bn.js": "4.11.6",
    "@types/fs-extra": "8.1.0",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.2",
    "@types/web3": "1.0.20",
    "bn.js": "4.11.8",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.1",
    "eth-ens-namehash": "2.0.8",
    "eth-sig-util": "2.5.3",
    "expect": "25.1.0",
    "fs-extra": "9.0.0",
    "ganache-cli": "6.10.1",
    "solhint": "3.2.2",
    "triple-beam": "1.3.0",
    "truffle": "5.5.0-tezos.4",
    "truffle-privatekey-provider": "1.2.0",
    "web3": "1.2.11",
    "winston": "3.2.1"
  }
}
